<odoo>
    <data>
        <record model="ir.ui.view" id="delivery_report_wiz_view">
            <field name="name">delivery_report_wiz_view</field>
            <field name="model">delivery.report.wiz</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <group>
                            <field name="start_date" required="1"/>
                            <field name="end_date" required="1"/>
                        </group>
                        <group>
                            <field name="partner_id" widget="many2many_tags" options="{'no_create': True, 'no_edit': True, 'no_open': True}"/>
                            <field name="analytic_account_id" widget="many2many_tags" options="{'no_create': True, 'no_edit': True, 'no_open': True}"/>
                            <field name="analytic_tag_id" widget="many2many_tags" options="{'no_create': True, 'no_edit': True, 'no_open': True}"/>
                        </group>
                    </group>
                    <footer>
                        <button string="Print" class="btn-primary"
                                name="%(inventory_reports_baykee.action_delivery_report)d" type="action"/>
                        <button string="Cancel" class="btn-danger" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>
        <record model="ir.actions.act_window" id="action_delivery_report_wiz_view">
            <field name="name">Deliveries Report Wizard</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">delivery.report.wiz</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="delivery_report_wiz_view"/>
            <field name="target">new</field>
        </record>
        <menuitem id="menu_delivery_report" name="Deliveries" parent="stock.menu_warehouse_report"
                  action="action_delivery_report_wiz_view" sequence="15"/>
    </data>
</odoo>