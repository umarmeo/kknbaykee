<odoo>
    <data>
        <record model="ir.ui.view" id="product_move_report_wiz_view">
            <field name="name">product_move_report_wiz_view</field>
            <field name="model">product.move.wiz</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <group>
                            <field name="start_date" required="1"/>
                            <field name="end_date" required="1"/>
                        </group>
                        <group>
                            <field name="product_id" options="{'no_create': True, 'no_edit': True, 'no_open': True}"
                                   widget="many2many_tags"/>
                        </group>
                    </group>
                    <footer>
                        <button string="Print" class="btn-primary"
                                name="%(inventory_reports_baykee.action_product_move_report)d" type="action"/>
                        <button string="Cancel" class="btn-danger" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>
        <record model="ir.actions.act_window" id="action_product_move_report_wiz_view">
            <field name="name">Product Move Report Wizard</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">product.move.wiz</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="product_move_report_wiz_view"/>
            <field name="target">new</field>
        </record>
        <menuitem id="menu_product_move_report" name="Product Move" parent="stock.menu_warehouse_report"
                  action="action_product_move_report_wiz_view" sequence="13"/>
    </data>
</odoo>