<odoo>
    <data>
        <record model="ir.ui.view" id="payment_status_report_wiz_view">
            <field name="name">payment_status_report_wiz_view</field>
            <field name="model">payment.status.wiz</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <group>
                            <field name="state"/>
                            <field name="division_id" widget="many2many_tags"
                                   options="{'no_create': True, 'no_edit': True, 'no_open': True}"/>
                            <field name="analytic_account_id" widget="many2many_tags"
                                   options="{'no_create': True, 'no_edit': True, 'no_open': True}"/>
                            <field name="payment_mode" widget="many2many_tags"
                                   options="{'no_create': True, 'no_edit': True, 'no_open': True}"/>
                        </group>
                    </group>
                    <footer>
                        <button string="Print" class="btn-primary"
                                name="%(payment_process_reports_baykee.action_payment_status_report)d" type="action"/>
                        <button string="Cancel" class="btn-danger" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>
        <record model="ir.actions.act_window" id="action_payment_status_report_wiz_view">
            <field name="name">Payment Status Report Wizard</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">payment.status.wiz</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="payment_status_report_wiz_view"/>
            <field name="target">new</field>
        </record>

        <menuitem id="payment_process_reporting" name="Reporting" parent="update_payment_process_module_baykee.payment_process_module_baykee_main_menu" sequence="2"/>
        <menuitem id="menu_payment_status_report" name="Payment Status" parent="payment_process_reporting"
                  action="action_payment_status_report_wiz_view" sequence="5"/>
    </data>
</odoo>