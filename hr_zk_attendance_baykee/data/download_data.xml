<?xml version="1.0"?>
<odoo>
	<record forcecreate="True" id="cron_download_data" model="ir.cron">
		<field name="name">Download Data</field>
		<field eval="True" name="active" />
		<field name="user_id" ref="base.user_root" />
		<field name="interval_number">1</field>
		<field name="interval_type">hours</field>
		<field name="numbercall">-1</field>
		<field name="model_id" ref="hr_zk_attendance_baykee.model_zk_machine" />
		<field name="state">code</field>
		<field name="code">model.cron_download()</field>
	</record>
	<record forcecreate="True" id="cron_add_data_absent" model="ir.cron">
		<field name="name">Absent/Rest days Data For Attendance</field>
		<field eval="True" name="active" />
		<field name="user_id" ref="base.user_root"/>
		<field name="interval_number">1</field>
		<field name="interval_type">days</field>
		<field name="numbercall">-1</field>
		<field name="model_id" ref="model_hr_attendance" />
		<field name="state">code</field>
		<field name="code">model.cron_download()</field>
	</record>
</odoo>
