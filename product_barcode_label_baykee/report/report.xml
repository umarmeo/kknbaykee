<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="product_barcode_label_paperformat" model="report.paperformat">
            <field name="name">Product Barcode Label Paperformat</field>
            <field name="format">custom</field>
            <field name="page_height">265</field>
            <field name="page_width">200</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">0</field>
            <field name="margin_bottom">0</field>
            <field name="margin_left">0</field>
            <field name="margin_right">0</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">0</field>
            <field name="dpi">90</field>
        </record>

        <record id="action_print_product_label" model="ir.actions.report">
            <field name="name">Print Product Label</field>
            <field name="model">product.label.baykee</field>
            <field name="print_report_name">'Barcode Label'</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">product_barcode_label_baykee.product_label_temp</field>
            <field name="report_file">product_barcode_label_baykee.product_label_temp</field>
            <field name="binding_model_id" ref="model_product_label_baykee"/>
            <field name="binding_type">report</field>
            <field name="paperformat_id" ref="product_barcode_label_paperformat"/>
        </record>

    </data>
</odoo>